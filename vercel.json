{
    "version": 2,
    "builds": [
        {
            "src": "medkit_record_management_system/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb","runtime": "python3.11.2"}
        },
        {
            "src":"build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes":[
        {
            "src":"/static/(.*)",
            "dest":"/static/$1"
        },
        {
            "src":"/(.*)",
            "dest":"medkit_record_management_system/wsgi.py"
        }
    ]
}